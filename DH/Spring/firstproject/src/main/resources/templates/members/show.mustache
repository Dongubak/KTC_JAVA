{{>layouts/header}}

<table class="table">
    <thead>
    <tr>
        <th scope="col">Id</th>
        <th scope="col">email</th>
        <th scope="col">password</th>
    </tr>
    </thead>
    <tbody>
    {{#memberArticle}}
    <tr>
        <th>{{id}}</th>
        <td>{{email}}</td>
        <td>{{password}}</td>
    </tr>
    {{/memberArticle}}
    </tbody>
</table>
<button type="submit" class="btn btn-primary" action="/members/{{memberArticle.id}}/edit">수정하기</button>
<a href="/members">Go to Member List</a>

{{>layouts/footer}}